                        <label for="Null" style="padding-right:5px">N/A</label>
                        <br />
                        <select multiple="multiple" name="browser" id="browser" style="width: 250px; padding-left: 10px;">
                            <option value="1">IE6</option>
                            <option value="2">IE7</option>
                            <option value="3">IE8</option>
                            <option value="4">IE9</option>
                            <option value="5">IE10</option>
                            <option value="6">IE11</option>
                            <option value="7">Firefox</option>
                            <option value="8">Safari</option>
                            <option value="9">Opera</option>
                            <option value="10">Chrome</option>
                            <option value="11">Edge</option>
                                
                        </select>


                        <h6 style="text-align: center;">
                            Press and hold ctrl to make multiple selections
                        </h6>

                        <br />
                    </div>
                    <br />
                    <div class="form-group">
                        @Html.Label("Files Included", new { @class = "control-label col-md-2" })
