                            <option value="1">Layered PSD</option>
                            <option value="2">Layered PNG</option>
                            <option value="3">HTML Files</option>
                            <option value="4">CSS Files</option>
                            <option value="5">JS Files</option>
                            <option value="6">Ecwid CSS Files</option>
                        </select>

                        <h6 style="text-align: center;">
                            Press and hold ctrl to make multiple selections
                        </h6>

                        <br />
                    </div>
                    <br />
                    <div class="form-group">
                        @Html.Label("Compatible With", new { @class = "control-label col-md-2" })
                        @Html.CheckBox("CompatibleWith", false, new { @checked = "checked", @class = "control-label col-md-1" });

                        <label for="Null" style="padding-right:5px">N/A</label>
                        <br />
                        <select multiple="multiple" name="compatiblewith" id="compatiblewith" style="width: 250px; padding-left: 10px;">
                            <option value="1">Ecwid</option>
                            <option value="2">Bootstrap 3.x</option>
                            <option value="3">Bootstrap 2.3.x</option>
                            <option value="4">Bootstrap 2.2.2</option>
                            <option value="5">Bootstrap 2.2.1</option>
                            <option value="6">Bootstrap 2.1.1</option>
                            <option value="7">Bootstrap 2.1.0</option>
                            <option value="8">Bootstrap 2.0.4</option>
                            <option value="9">Bootstrap 2.0.3</option>
                            <option value="10">Bootstrap 2.0.2</option>
                            <option value="11">Bootstrap 2.0.1</option>
                            <option value="12">Bootstrap 2.0</option>
                            <option value="13">Foundation 6</option>
                            <option value="14">Foundation 5</option>
                            <option value="15">Foundation 4</option>
                        </select>

                        <h6 style="text-align: center;">
                            Press and hold ctrl to make multiple selections
                        </h6>

                        <br />
                        


                        <div class="form-horizontal" style="padding: 15px;">
                            <div class="form-group">
                                @Html.Label("Support", new { @class = "control-label col-md-2" })
                                <div class="col-md-10 " style="padding-top: 10px;">
                                    Yes: <input checked="checked"
                                                id="support"
                                                name="support"
                                                type="radio"
                                                value="True" />

                                    No: <input id="support"
                                               name="support"
                                               type="radio"
                                               value="False" />

                                </div>

                            </div>
                        </div>
                        <br />

                        <div class="form-group">
                            @Html.LabelFor(model => model.DemoURL, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.DemoURL, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.DemoURL, "", new { @class = "text-danger" })


                            </div>
                        </div>

                        <br />


                        <div class="form-group">
                            @Html.LabelFor(model => model.Columns, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">


                                <select name="Columns">
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="3">3</option>
                                    <option value="4+">4+</option>
                                </select>

                            </div>
                        </div>


                        <br />

                        <div class="form-group">
                            @Html.LabelFor(model => model.Layout, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">

                                <select name="Layout" id="Layout" aria-invalid="false">
                                    <option value="Fixed">Fixed</option>
                                    <option value="Liquid">Liquid</option>
                                    <option value="Responsive">Responsive</option>
                                    <option value="N/A">N/A</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <br />
                    <br />
                    <p>Some of our fields allow you to select multiple values. Please use the CTRL key (PC) or COMMAND key (Mac) to select multiple items.</p>
                </div>

            </div>

        </div>
        <div class="col-lg-5">
            <div class="panel panel-default" id="content-formatting">
                <div class="panel-heading" style="background-color: #454545">
                    <h5 style="color: #fdf5e6;  text-align: center; ">Attributes</h5>
                </div>
                <div style="padding-left: 10px; padding-right: 10px;">
                    <br />
                    <p>You should tell which scanners the extension of the project and the file version you have installed are working.</p>

                    <br />
                </div>


            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-7">
            <div class="panel panel-default" id="content-formatting " style="background-color: #f5f5f5">
                <h3 style="color: #454545; padding-left: 10px;">Tags </h3>
                <br />
                <div class="form-horizontal" style="padding: 15px;">
                    <div class="form-group">
                        @Html.Label("Tags  ", htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-10">

                            @Html.TextAreaFor(model => model.Tags, new { htmlAttributes = new { @class = "resizer" }, cols = "150", rows = "8" })

                            @Html.ValidationMessageFor(model => model.Tags, "", new { @class = "text-danger" })
                            <br />
                            <div class="col-md-10">
                                <small style="padding-right: 10px;">
                                    Maximum of 15 keywords covering features, options and styling. Keywords should all be in lowercase and separated by commas. e.g. slider, gallery, newsletter, wordpress menu
                                </small>
                            </div>
                        </div>
                    </div>

                </div>

