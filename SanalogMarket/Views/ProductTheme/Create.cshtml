                            <label for="regular-price" style="font-size: 13px; padding-right: 20px;">Regular License</label>
                            <label for="regular-price" style="font-size: 17px;">$</label>
                            @Html.EditorFor(model => model.Price, new { htmlAttributes = new { @class = "form-control" }, })
                            <label for="regular-price" style="font-size: 17px; padding-left: 10px;">+</label>
                            <label for="regular-price" style="font-size: 13px; padding-left: 10px;color: #82b546">Buyer Free</label>
                            <label for="regular-price" style="font-size: 17px; padding-left: 10px;padding-right: 10px;color: #82b546">$4</label>
                            @Html.ValidationMessageFor(model => model.Price, "", new { @class = "text-danger" })
                        </div>

                    </div>

                </div>
                <hr style="border-color: #e1e8ed" />
                <div class="form-horizontal" style="padding: 15px;">
                    <div class="form-group">
                        @*                            @Html.Label("Regular License", htmlAttributes: new { @class = "control-label col-md-2" })*@
                        <div class="col-md-10 ">
                            <label for="regular-price" style="font-size: 13px; padding-right: 20px;">Extended License</label>
                            <label for="regular-price" style="font-size: 17px;">$</label>
                            @Html.EditorFor(model => model.Price, new { htmlAttributes = new { @class = "form-control" }, })
                            <label for="regular-price" style="font-size: 17px; padding-left: 10px;">+</label>
                            <label for="regular-price" style="font-size: 13px; padding-left: 10px;color: #82b546">Buyer Free</label>
                            <label for="regular-price" style="font-size: 17px; padding-left: 10px;color: #82b546">$20</label>
                            @Html.ValidationMessageFor(model => model.Price, "", new { @class = "text-danger" })
                        </div>

                    </div>

                </div>


            </div>

        </div>

    </div>
    <div class="row">
        <div class="col-lg-7">
            <div class="panel panel-default" id="content-formatting " style="background-color: #f5f5f5">
                <h3 style="color: #454545; padding-left: 10px;">Message to the Reviewer </h3>
                <br />
                <div class="form-horizontal" style="padding: 15px;">
                    <div class="form-group">
                        @Html.Label("Comments  ", htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-10">

                            @Html.TextAreaFor(model => model.Comment, new { htmlAttributes = new { @class = "resizer" }, cols = "150", rows = "8" })

                            @Html.ValidationMessageFor(model => model.Comment, "", new { @class = "text-danger" })
                            <br />
                            <br />
                            <div class="col-md-10">

                                @Html.CheckBox("imza", false, new { @checked = "checked", @class = "control-label col-md-1" });
                                <small style="padding-right: 10px;">
                                    Any images, sounds, video, code, flash, or other assets that are not my own work, have been appropriately licensed for use in the file preview or main download. Other than these items, this work is entirely my own and I have full rights to sell it on SanalogCode.
                                </small>
                                <br />

                                <br />

                            </div>
                        </div>

                    </div>
                            
                            
                  

                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Publish Product" class="btn btn-success " />
                            </div>
                        </div>

                    </div>

                </div>

            </div>

        </div>


    </div>
    
